{
	"name": "node-red-contrib-parser-ini",
	"version": "0.1.0",
	"description": "Node-RED node to parser/serialize INI configuration files",
	"readmeFilename": "readme.md",
	"keywords": [
		"ini",
		"configuration",
		"parser",
		"serializer",
		"reader",
		"writer"
	],
	"node-red": {
		"nodes": {
			"parser-ini": "parser-ini.js"
		}
	},
	"author": {
		"name": "Alexandre Alapetite",
		"url": "https://alexandra.dk/alexandre.alapetite"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/alexandrainst/node-red-contrib-parser-ini.git"
	},
	"dependencies": {
		"ini": "^1.3.5"
	},
	"devDependencies": {
		"jshint": "^2.11.1"
	},
	"jshintConfig": {
		"esversion": 8,
		"node": true,
		"multistr": true,
		"sub": true
	},
	"scripts": {
		"start": "node ./index.js",
		"lint": "node ./node_modules/.bin/jshint *.js",
		"pretest": "npm run-script lint",
		"test": "npm -v"
	}
}
